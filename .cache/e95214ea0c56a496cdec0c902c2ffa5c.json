olumn":14}},{"generated":{"line":100,"column":25},"source":"stopwatch.js","original":{"line":81,"column":0}},{"name":"stopwatch","generated":{"line":100,"column":26},"source":"stopwatch.js","original":{"line":81,"column":26}},{"name":"elem","generated":{"line":100,"column":36},"source":"stopwatch.js","original":{"line":81,"column":36}},{"generated":{"line":100,"column":40},"source":"stopwatch.js","original":{"line":81,"column":0}},{"name":"console","generated":{"line":101,"column":0},"source":"stopwatch.js","original":{"line":82,"column":0}},{"name":"log","generated":{"line":101,"column":8},"source":"stopwatch.js","original":{"line":82,"column":8}},{"generated":{"line":101,"column":11},"source":"stopwatch.js","original":{"line":82,"column":0}},{"name":"stopwatch","generated":{"line":101,"column":12},"source":"stopwatch.js","original":{"line":82,"column":12}},{"name":"time","generated":{"line":101,"column":22},"source":"stopwatch.js","original":{"line":82,"column":22}},{"generated":{"line":101,"column":26},"source":"stopwatch.js","original":{"line":82,"column":12}},{"generated":{"line":101,"column":28},"source":"stopwatch.js","original":{"line":82,"column":0}}],"sources":{"stopwatch.js":"class Stopwatch {\r\n    constructor() {\r\n        this.elem = this.display();\r\n        this.times = this.time();\r\n        this.createButtons();\r\n        this.handleButtons();\r\n        this.counter();\r\n    }\r\n\r\n    start() {\r\n        setInterval(() => {\r\n            this.times.miliseconds++\r\n            this.counter();\r\n        }, 100)\r\n    }\r\n\r\n    counter() {\r\n        const span = document.createElement('span');\r\n        this.elem.appendChild(span);\r\n        this.elem.childNodes[5].innerHTML = `<br>\r\n            ${this.addZero(this.times.minutes)}:${this.addZero(this.times.seconds)}:${this.addZero(this.times.miliseconds)}`\r\n    }\r\n\r\n    time() {\r\n        const time = {\r\n            miliseconds: 0,\r\n            seconds: 0,\r\n            minutes: 0\r\n        }\r\n        return time\r\n    }\r\n\r\n    handleButtons() {\r\n        this.elem.childNodes[0].onclick = () => {\r\n            this.start();\r\n        }\r\n    }\r\n\r\n    addZero(value) {\r\n        let result = value.toString();\r\n        if (result.length < 2) {\r\n            result = '0' + result\r\n\r\n        }\r\n        return result\r\n    }\r\n\r\n    createButtons() { //we will refer to elements by nodes e.g this.elem.children[0] //this.elem.childNodes[length]\r\n        const buttonStart = document.createElement('button');\r\n        const buttonStop = document.createElement('button');\r\n        const buttonLop = document.createElement('button');\r\n        const buttonClear = document.createElement('button');\r\n        const buttonClearLop = document.createElement('button');\r\n        buttonStart.textContent = 'Start';\r\n        buttonStop.textContent = 'stop';\r\n        buttonLop.textContent = 'Lop';\r\n        buttonClear.textContent = 'Clear All';\r\n        buttonClearLop.textContent = 'Clear Lop';\r\n        buttonStart.setAttribute('id', 'btn_start');\r\n        buttonStop.setAttribute('id', 'btn_stop');\r\n        buttonLop.setAttribute('id', 'btn_lop');\r\n        buttonClear.setAttribute('id', 'btn_clear');\r\n        buttonClearLop.setAttribute('id', 'btn_clearLop');\r\n        this.elem.appendChild(buttonStart);\r\n        this.elem.appendChild(buttonStop);\r\n        this.elem.appendChild(buttonLop);\r\n        this.elem.appendChild(buttonClear);\r\n        this.elem.appendChild(buttonClearLop);\r\n    }\r\n\r\n    display() {\r\n        const container = document.createElement('div');\r\n        container.setAttribute('id', 'container_id');\r\n\r\n        return container\r\n    }\r\n}\r\n\r\n\r\nconst stopwatch = new Stopwatch();\r\ndocument.body.appendChild(stopwatch.elem);\r\nconsole.log(stopwatch.time())\r\n"},"lineCount":null}},"hash":"666eb90a0c2ca4b54719ad659774a8f2","cacheData":{"env":{}}}